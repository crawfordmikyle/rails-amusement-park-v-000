<%= form_for @user do |f| %>
  <div class="form-group">
    <%= f.label :name, class: "control-label"  %>
    <%= f.text_field :name, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :password, class: "control-label"  %>
    <%= f.text_field :password, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :happiness, class: "control-label"  %>
    <%= f.text_field :happiness, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :tickets, class: "control-label"  %>
    <%= f.text_field :tickets, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :height, class: "control-label"  %>
    <%= f.text_field :height, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :nausea, class: "control-label"  %>
    <%= f.text_field :nausea, class: "form-control" %>
  </div>
  <div class="check_box">
    <%= f.label :admin, class: "control-label"  %>
    <%= f.check_box :admin %>
  </div>

  <div class="form-group">
    <%= f.submit class: "btn btn-default btn-primary" %>
  </div>
<% end %>
